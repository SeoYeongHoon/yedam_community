<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yedam.app.yedam_cbt.mapper.CbtStudentMapper">
	<!-- 전체시험리스트 출력 -->
	<select id="selectTestListAll" resultType="TestVO">
		SELECT test_id
		       ,curriculum_id
		       ,test_content
		       ,test_time
		       ,test_date
		       ,test_name
		       ,class_id
		FROM tests
		ORDER BY test_id
	</select>
</mapper>